<form>
    <group id="header" colspan="4" col="6">
        <label name="reference"/>
        <field name="reference"/>
        <label name="date"/>
        <field name="date"/>
        <label name="company"/>
        <field name="company"/>
    </group>
    <label name="from_warehouse"/>
    <field name="from_warehouse"/>
    <label name="to_warehouse"/>
    <field name="to_warehouse"/>
    <notebook>
        <page name="lines">
            <field name="lines" colspan="4"/>
        </page>
        <page name="note">
            <field name="note" colspan="4"/>
        </page>
    </notebook>
    <group id="state_buttons" colspan="4">
        <label name="state"/>
        <field name="state"/>
        <button name="confirm" string="Confirm" icon="tryton-go-next"/>
    </group>
</form>
